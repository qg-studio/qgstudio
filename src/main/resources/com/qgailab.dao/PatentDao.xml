<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qgailab.dao.PatentDao">

    <select id="getPatentById" parameterType="java.math.BigInteger" resultType="com.qgailab.dao.PatentDao">
        select number,type,name,zl,inventor,gmt_create,gmt_modified from patent where id = #{id}
    </select>

    <delete id="removePatentById" parameterType="java.math.BigInteger">
        delete from patent where id = #{id}
    </delete>


</mapper>